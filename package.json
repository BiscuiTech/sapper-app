{
  "name": "biscuitech-personal-website",
  "description": "TODO",
  "version": "0.0.1",
  "scripts": {
    "watch:tailwind": "postcss static/tailwind.css -o static/index.css -w",
    "build:tailwind": "NODE_ENV=production postcss static/tailwind.css -o static/index.css",
    "dev": "sapper dev",
    "build": "npm run build:tailwind && sapper export",
    "prod": "sapper export; npx serve __sapper__/export",
    "export": "sapper export",
    "start": "node __sapper__/build",
    "cy:run": "cypress run --headed",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "jest": "jest --watchAll"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "4.0.0",
    "@tailwindcss/ui": "0.7.2",
    "compression": "1.7.4",
    "dark-mode-toggle": "0.8.0",
    "date-fns": "2.17.0",
    "gray-matter": "4.0.2",
    "highlight.js": "10.6.0",
    "marked": "2.0.0",
    "netlify-cms": "2.10.93",
    "polka": "0.5.2",
    "prismjs": "1.23.0",
    "reading-time": "1.3.0",
    "sirv": "1.0.11",
    "svelte-i18n": "3.3.4"
  },
  "devDependencies": {
    "@babel/core": "7.12.17",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.12.17",
    "@babel/preset-env": "7.12.17",
    "@babel/runtime": "7.12.18",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "11.2.0",
    "@rollup/plugin-replace": "2.3.4",
    "@storybook/addon-actions": "6.1.18",
    "@storybook/addon-essentials": "6.1.18",
    "@storybook/addon-links": "6.1.18",
    "@storybook/svelte": "6.1.18",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/svelte": "3.0.3",
    "babel-loader": "8.2.2",
    "core-js": "3.9.0",
    "cypress": "6.5.0",
    "eslint-plugin-svelte3": "3.1.2",
    "jest": "26.6.3",
    "jest-transform-svelte": "2.1.1",
    "npm-run-all": "4.1.5",
    "postcss-cli": "8.3.1",
    "react-is": "17.0.1",
    "rollup": "2.39.0",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-glob": "1.0.2",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-svelte": "7.1.0",
    "rollup-plugin-terser": "7.0.2",
    "sapper": "0.29.1",
    "svelte": "3.32.3",
    "svelte-loader": "3.0.0",
    "tailwindcss": "2.0.3"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": "jest-transform-svelte"
    },
    "moduleFileExtensions": [
      "js",
      "svelte",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss|stylesheet)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/storybook-static/",
      "/cypress/"
    ]
  }
}
